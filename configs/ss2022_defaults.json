{
  "method": "2022-blake3-aes-256-gcm",
  "password": "AUTO_GENERATE",
  "network": "tcp,udp",
  "plugin": "v2ray-plugin",
  "plugin_opts": {
    "mode": "websocket",
    "tls": true,
    "host": "${PANEL_DOMAIN}",
    "path": "/ss2022"
  },
  "ports": [8388, 8443],
  "encryption": {
    "cipher": "aes-256-gcm",
    "key_derivation": "blake3"
  },
  "features": {
    "session_udp": true,
    "eih": true,
    "multicast": false
  },
  "timeout": {
    "handshake": 4,
    "idle": 300
  },
  "relay": {
    "enabled": true,
    "timeout": 30
  },
  "dns": {
    "servers": ["1.1.1.1", "8.8.8.8"],
    "fallback": ["9.9.9.9"]
  }
}
